{
  "name": "vite-frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "serve": "vite",
    "build": "vite build",
    "build:watch": "vite build --watch --base=/app/",
    "preview": "vite preview",
    "prebuild": "node ./component-update.js",
    "build:formio": "node ./component-update.js build",
    "deploy:formio": "node ./component-update.js deploy",
    "lint": "eslint \"src/**/*.{js,vue}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,vue}\"",
    "prettier": "prettier --check \"src/**/*.{js,vue,css,scss}\"",
    "prettier:fix": "prettier --write \"src/**/*.{js,vue,css,scss}\"",
    "clean": "rm -rf coverage dist **/e2e/videos",
    "clean:formio": "node ./component-update.js clean",
    "purge": "rm -rf src/formio node_modules",
    "purge:formio": "node ./component-update.js purge",
    "rebuild": "npm run clean && npm run build",
    "reinstall": "npm run purge && npm install"
  },
  "dependencies": {
    "@bcgov/bc-sans": "^1.0.1",
    "@formio/vue": "github:formio/vue#v5.0.0-rc.2",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "axios": "^1.4.0",
    "bootstrap-scss": "^5.2.3",
    "fast-json-patch": "^3.1.1",
    "font-awesome": "^4.7.0",
    "formiojs": "^4.14.13",
    "keycloak-js": "^21.1.1",
    "mitt": "^3.0.0",
    "moment": "^2.29.4",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.1",
    "vue": "^3.2.47",
    "vue-json-pretty": "^2.2.4",
    "vue-router": "^4.2.0",
    "vue3-clipboard": "^1.0.0",
    "vuetify": "^3.2.5"
  },
  "devDependencies": {
    "@mdi/font": "^7.2.96",
    "@vitejs/plugin-vue": "^4.1.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.13.0",
    "eslint-plugin-vuetify": "^2.0.0-beta.4",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "terser": "^5.17.4",
    "vite": "^4.3.2",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-vuetify": "^1.0.2"
  }
}
