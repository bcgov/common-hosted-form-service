{
  "name": "common-hosted-form-service-frontend",
  "version": "1.2.0",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "serve": "vite serve",
    "preserve": "node ./component-update.js",
    "build": "vite build",
    "prebuild": "node ./component-update.js",
    "build:formio": "node ./component-update.js build",
    "deploy:formio": "node ./component-update.js deploy",
    "test": "npm run test:unit",
    "test:unit": "vite test:unit --verbose --forceExit --detectOpenHandles",
    "lint": "eslint 'src/**/*.{js,vue}'",
    "lint:fix": "eslint --fix 'src/**/*.{js,vue}'",
    "prettier": "prettier --check 'src/**/*.{js,vue,css,scss}'",
    "prettier:fix": "prettier --write 'src/**/*.{js,vue,css,scss}'",
    "pretest": "npm run lint",
    "clean": "rm -rf coverage dist **/e2e/videos",
    "clean:formio": "node ./component-update.js clean",
    "purge": "rm -rf src/formio node_modules",
    "purge:formio": "node ./component-update.js purge",
    "rebuild": "npm run clean && npm run build",
    "reinstall": "npm run purge && npm install"
  },
  "dependencies": {
    "@bcgov/bc-sans": "^1.0.1",
    "@formio/vue": "github:formio/vue#v5.0.0-rc.2",
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@mdi/font": "^6.9.96",
    "axios": "^0.27.2",
    "bootstrap-scss": "^4.6.1",
    "fast-json-patch": "^3.1.1",
    "font-awesome": "^4.7.0",
    "formiojs": "^4.14.6",
    "keycloak-js": "^15.0.2",
    "material-design-icons-iconfont": "^6.7.0",
    "moment": "^2.29.3",
    "nprogress": "^0.2.0",
    "qrcode.vue": "^3.4.0",
    "uuid": "^8.3.2",
    "vue": "^3.2.47",
    "vue-json-pretty": "^1.8.3",
    "vue-router": "^4.1.6",
    "vue3-clipboard": "^1.0.0",
    "vuetify": "^3.2.1",
    "vuex": "^4.1.0",
    "vuex-map-fields": "^1.4.1"
  },
  "devDependencies": {
    "@vitejs/plugin-legacy": "^4.0.3",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vue/test-utils": "^2.3.2",
    "axios-mock-adapter": "^1.21.1",
    "eslint": ">=8.39.0 <9.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": ">=8.7.1 <9.0.0",
    "lodash": "^4.17.21",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "terser": "^5.17.1",
    "vite": "^4.3.3",
    "vite-plugin-eslint": "^1.8.1",
    "vue-cli-plugin-vuetify": "^2.5.1",
    "vue-eslint-parser": "^9.1.1"
  }
}
