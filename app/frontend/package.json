{
  "name": "common-hosted-form-service-frontend",
  "version": "1.5.0",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "serve": "vite",
    "preserve": "node ./scripts/component-update.js",
    "build": "vite build",
    "build:watch": "vite build --watch",
    "build:embed": "npm run build:embed:viewer && npm run build:embed:script",
    "build:embed:viewer": "terser public/embed/chefs-form-viewer.js -o dist/embed/chefs-form-viewer.min.js --source-map \"content=inline,includeSources\"",
    "build:embed:script": "terser public/embed/chefs-form-viewer-embed.js -o dist/embed/chefs-form-viewer-embed.min.js --source-map \"content=inline,includeSources\"",
    "preview": "vite preview",
    "prebuild": "node ./scripts/component-update.js",
    "build:formio": "node ./scripts/component-update.js build",
    "deploy:formio": "node ./scripts/component-update.js deploy",
    "test": "npm run test:unit",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "lint": "eslint \"src/**/*.{js,vue}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,vue}\"",
    "prettier": "prettier --check \"src/**/*.{js,vue,css,scss}\"",
    "prettier:fix": "prettier --write \"src/**/*.{js,vue,css,scss}\"",
    "pretest": "npm run lint",
    "clean": "rm -rf coverage dist **/e2e/videos && npm run clean:embed",
    "clean:embed": "rm -f dist/embed/*.min.js dist/embed/*.min.js.map",
    "clean:formio": "node ./scripts/component-update.js clean",
    "purge": "rm -rf src/formio node_modules",
    "purge:formio": "node ./scripts/component-update.js purge",
    "rebuild": "npm run clean && npm run build",
    "reinstall": "npm run purge && npm install",
    "rebuild:formio": "npm run build:formio && npm run deploy:formio && npm run dev",
    "dev": "vite --port 5173 --host"
  },
  "dependencies": {
    "@bcgov/bc-sans": "^1.0.1",
    "@formio/vue": "^5.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@vueuse/core": "^11.1.0",
    "axios": "^1.9.0",
    "bootstrap-scss": "^5.3.1",
    "crypto-js": "^4.1.1",
    "fast-json-patch": "^3.1.1",
    "font-awesome": "^4.7.0",
    "formiojs": "4.17.4",
    "keycloak-js": "^26.2.0",
    "leaflet": "^1.9.4",
    "leaflet-draw": "^1.0.4",
    "leaflet-geosearch": "^4.0.0",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "moment": "^2.29.4",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.4",
    "qrcode.vue": "^3.4.0",
    "uuid": "^9.0.0",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-json-pretty": "^2.2.4",
    "vue-router": "^4.2.2",
    "vue3-clipboard": "^1.0.0",
    "vuetify": "^3.5.14"
  },
  "devDependencies": {
    "@mdi/font": "^7.2.96",
    "@pinia/testing": "^0.1.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^4.0.17",
    "@vue/test-utils": "^2.3.2",
    "axios-mock-adapter": "^1.21.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.13.0",
    "eslint-plugin-vuetify": "^2.0.0-beta.4",
    "happy-dom": "^20.1.0",
    "jsdom": "^22.1.0",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "terser": "^5.17.4",
    "vite": "^6.2.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-vuetify": "^2.0.3",
    "vitest": "^4.0.17",
    "vitest-canvas-mock": "^1.1.3"
  }
}
