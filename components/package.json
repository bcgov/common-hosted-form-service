{
  "name": "@bcgov/formio",
  "version": "1.0.0",
  "description": "bcgov components for form.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bcgov/common-hosted-form-service.git"
  },
  "author": "Forminators <submit.digital@gov.bc.ca>",
  "license": "Apache-2.0",
  "bugs": {
