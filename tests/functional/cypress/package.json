{
  "name": "chefs-cypress",
  "version": "1.0.0",
  "private": true,
  "description": "Cypress functional test suite for CHEFS",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf **/screenshots **/videos **/axe-reports",
    "purge": "rm -rf node_modules",
    "test": "cypress run",
    "test:allbrowsers": "npm run test:chrome && npm run test:edge && npm run test:firefox",
    "test:chrome": "npm run test -- --browser chrome",
    "test:dev": "cypress open",
    "test:edge": "npm run test -- --browser edge",
    "test:firefox": "npm run test -- --browser firefox"
  },
  "keywords": [
    "chefs",
    "cypress"
  ],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "date-fns": "^2.26.0"
  },
  "devDependencies": {
    "axe-core": "^4.11.0",
    "cypress": "14.2.0",
    "cypress-axe": "^1.7.0",
    "cypress-axe-reporter": "^1.3.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-keycloak-commands": "^1.2.0"
  }
}
